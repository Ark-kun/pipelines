{
  "pipelineInfo": {
    "name": "two-step-pipeline-with-ontology"
  },
  "tasks": [
    {
      "taskInfo": {
        "name": "Ingestion"
      },
      "inputs": {
        "parameters": {
          "input_location": {
            "runtimeValue": {
              "runtimeParameter": "input_location"
            }
          }
        }
      },
      "outputs": {
        "artifacts": {
          "schema": {
            "artifactType": {
              "instanceSchema": "title: kfp.Schema\ntype: object\nproperties:\n"
            }
          },
          "examples": {
            "artifactType": {
              "instanceSchema": "title: kfp.Dataset\ntype: object\nproperties:\n"
            }
          }
        }
      },
      "executorLabel": "Ingestion"
    },
    {
      "taskInfo": {
        "name": "Trainer"
      },
      "inputs": {
        "parameters": {
          "n_epochs": {
            "runtimeValue": {
              "runtimeParameter": "n_epochs"
            }
          },
          "optimizer": {
            "runtimeValue": {
              "runtimeParameter": "optimizer"
            }
          }
        },
        "artifacts": {
          "schema": {
            "producerTask": "Ingestion",
            "outputArtifactKey": "schema"
          },
          "examples": {
            "producerTask": "Ingestion",
            "outputArtifactKey": "examples"
          }
        }
      },
      "outputs": {
        "artifacts": {
          "model": {
            "artifactType": {
              "instanceSchema": "title: kfp.Model\ntype: object\nproperties:\n"
            }
          },
          "metrics": {
            "artifactType": {
              "instanceSchema": "title: kfp.Metrics\ntype: object\nproperties:\n"
            }
          }
        }
      },
      "executorLabel": "Trainer"
    }
  ],
  "deploymentConfig": {
    "@type": "type.googleapis.com/ml_pipelines.PipelineDeploymentConfig",
    "executors": {
      "Ingestion": {
        "container": {
          "image": "gcr.io/my-project/my-ingestor",
          "args": [
            "--input-location",
            "{{$.inputs.parameters['input_location']}}",
            "--output-examples",
            "{{$.outputs.artifacts['examples'].path}}",
            "--output-schema",
            "{{$.outputs.artifacts['schema'].path}}"
          ]
        }
      },
      "Trainer": {
        "container": {
          "image": "gcr.io/my-project/my-fancy-trainer",
          "args": [
            "--input-examples",
            "{{$.inputs.artifacts['examples'].path}}",
            "--input-schema",
            "{{$.inputs.artifacts['schema'].path}}",
            "--optimizer",
            "{{$.inputs.parameters['optimizer']}}",
            "--n_epochs",
            "{{$.inputs.parameters['n_epochs']}}",
            "--output-model",
            "{{$.outputs.artifacts['model'].path}}",
            "--output-metrics",
            "{{$.outputs.artifacts['metrics'].path}}"
          ]
        }
      }
    }
  },
  "sdkVersion": "kfp-1.0.1",
  "schemaVersion": "v2alpha1",
  "runtimeParameters": {
    "optimizer": {
      "type": "STRING",
      "defaultValue": {
        "stringValue": "sgd"
      }
    },
    "n_epochs": {
      "type": "STRING",
      "defaultValue": {
        "stringValue": "200"
      }
    },
    "input_location": {
      "type": "STRING",
      "defaultValue": {
        "stringValue": "gs://test-bucket/pipeline_root"
      }
    }
  }
}