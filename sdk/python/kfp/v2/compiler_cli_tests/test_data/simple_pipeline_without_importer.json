{
  "pipelineInfo": {
    "name": "simple-two-step-pipeline"
  },
  "tasks": [
    {
      "taskInfo": {
        "name": "Write"
      },
      "inputs": {
        "parameters": {
          "text": {
            "runtimeValue": {
              "runtimeParameter": "text"
            }
          }
        }
      },
      "outputs": {
        "artifacts": {
          "data": {
            "artifactType": {
              "instanceSchema": "title: kfp.Dataset\ntype: object\nproperties:\n"
            }
          }
        }
      },
      "executorLabel": "Write"
    },
    {
      "taskInfo": {
        "name": "Read"
      },
      "inputs": {
        "artifacts": {
          "data": {
            "producerTask": "Write",
            "outputArtifactKey": "data"
          }
        }
      },
      "executorLabel": "Read"
    }
  ],
  "deploymentConfig": {
    "@type": "type.googleapis.com/ml_pipelines.PipelineDeploymentConfig",
    "executors": {
      "Write": {
        "container": {
          "image": "google/cloud-sdk:slim",
          "command": [
            "sh",
            "-c",
            "set -e -x\necho \"$0\" > \"$1\"\n",
            "{{$.inputs.parameters['text']}}",
            "{{$.outputs.artifacts['data'].path}}"
          ]
        }
      },
      "Read": {
        "container": {
          "image": "google/cloud-sdk:slim",
          "command": [
            "sh",
            "-c",
            "set -e -x\ncat \"$0\"\n",
            "{{$.inputs.artifacts['data'].path}}"
          ]
        }
      }
    }
  },
  "sdkVersion": "kfp-1.0.1",
  "schemaVersion": "v2alpha1",
  "runtimeParameters": {
    "text": {
      "type": "STRING",
      "defaultValue": {
        "stringValue": "Hello world!"
      }
    }
  }
}
